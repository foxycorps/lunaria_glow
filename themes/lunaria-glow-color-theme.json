{
  "$schema": "vscode://schemas/color-theme",
  "name": "Lunaria Glow",
  "type": "dark",
  "colors": {
    "editor.background": "#111111",
    "editor.foreground": "#eeeee1",
    "editor.selectionBackground": "#333333",
    "editor.lineHighlightBackground": "#1a1a1a",
    "editorCursor.foreground": "#ffb84a",
    "editorIndentGuide.background1": "#222222",
    "editorIndentGuide.activeBackground1": "#444444",
    "editorLineNumber.foreground": "#555555",
    "editorLineNumber.activeForeground": "#eeeee1",
    "editorWhitespace.foreground": "#2a2a2a",
    "editorBracketMatch.background": "#222222",
    "editorBracketMatch.border": "#6d89ff",

    "editorError.foreground": "#de7176",
    "editorWarning.foreground": "#ffb84a",
    "editorInfo.foreground": "#6d89ff",

    "diffEditor.insertedTextBackground": "#76c49040",
    "diffEditor.removedTextBackground": "#de717640",

    "sideBar.background": "#111111F2",
    "sideBar.foreground": "#eeeee1cc",
    "sideBarTitle.foreground": "#eeeee1cc",
    "sideBarSectionHeader.foreground": "#eeeee1cc",
    "activityBar.background": "#111111F2",
    "activityBar.foreground": "#eeeee1cc",
    "statusBar.background": "#111111F2",
    "statusBar.foreground": "#eeeee1cc",
    "panel.background": "#111111",
    "panel.border": "#222222",
    "tab.activeBackground": "#1a1a1a",
    "tab.inactiveBackground": "#111111",
    "tab.activeForeground": "#eeeee1",
    "tab.inactiveForeground": "#999999"
  },
  "tokenColors": [
    {
      "settings": { "foreground": "#eeeee1" }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#999999", "fontStyle": "italic" }
    },
    {
      "scope": ["keyword", "storage", "entity.name.tag", "punctuation.accessor", "keyword.operator"],
      "settings": { "foreground": "#6d89ff" }
    },
    {
      "scope": ["entity.other.attribute-name", "meta.attribute", "variable.parameter.attr-name"],
      "settings": { "foreground": "#6d89ff" }
    },
    {
      "scope": [
        // JSON / JSONC property keys
        "meta.structure.dictionary.json string.quoted.double.json",
        "meta.structure.dictionary.json string.quoted.single.json",
        "support.type.property-name.json",
        "support.type.property-name.json.comment",
        // JavaScript / TypeScript object-literal keys
        "meta.object-literal.key.js string.quoted.double.js",
        "meta.object-literal.key.cjs string.quoted.double.js",
        "meta.object-literal.key.jsx string.quoted.double.jsx",
        "meta.object-literal.key.ts string.quoted.double.ts",
        "meta.object-literal.key.tsx string.quoted.double.tsx"
      ],
      "settings": { "foreground": "#6d89ff" }
    },
    {
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "meta.attribute-value",
        "string.other.link",
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": { "foreground": "#a36c8c" }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "entity.name.type",
        "entity.name.function",
        "support.function",
        "support.type.builtin",
        "variable.language",
        "variable.other.constant"
      ],
      "settings": { "foreground": "#ffb84a" }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "constant.other.symbol",
        "variable.other.readwrite",
        "variable.other.object",
        "variable.other.property",
        "variable",
        "meta.embedded"
      ],
      "settings": { "foreground": "#b3d77e" }
    },
    {
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.parameters",
        "meta.brace",
        "meta.bracket"
      ],
      "settings": { "foreground": "#cccccc" }
    },
    {
      "scope": ["markup.heading", "markup.bold"],
      "settings": { "foreground": "#ffb84a", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.italic"],
      "settings": { "foreground": "#999999", "fontStyle": "italic" }
    },
    {
      "scope": [
        "markup.deleted",
        "diff.deleted",
        "meta.diff.header.from-file",
        "meta.diff.range.from"
      ],
      "settings": { "foreground": "#de7176" }
    },
    {
      "scope": [
        "markup.inserted",
        "diff.inserted",
        "meta.diff.header.to-file",
        "meta.diff.range.to"
      ],
      "settings": { "foreground": "#76c490" }
    }
  ]
}
